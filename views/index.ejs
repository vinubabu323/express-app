<!-- include header -->
<%-include('include/_header.ejs')%>
<!-- include header -->
<main id="site-main">
  <div class="container">
    <div class="box-nav d-flex-justify-between">
      <a href="/add-user" class="border-shadow">
        <span class="text-gradient">
          New User <i class="fas fa-user"></i>
        </span>
      </a>
      <a href="/login-user" class="border-shadow">
        <span class="text-gradient">
         Login <i class="fas fa-user"></i>
        </span>
      </a>
    </div>
    <form action="/" method="post">
      <table class="table">
        <thead class="thead-dark">
          <tr>
            <th>Id</th>
            <th>Name</th>
            <th>Image</th>
            <th>Email</th>
            <th>Gender</th>
            <th>Status</th>
            <th>Action</th>
          </tr>
        </thead>
        <tbody>
          <%- include('include/_show.ejs')%>
        </tbody>
      </table>
    </form>
  </div>
</main>
<!-- include footer -->
<%-include('./include/_footer.ejs')%>
